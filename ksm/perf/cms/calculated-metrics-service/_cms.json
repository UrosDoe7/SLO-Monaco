{
  "tsmMetricKey": "{{ .name }}",
  "name": "{{ .name }}",
  "enabled": true,
  "metricDefinition": {
    "metric": "REQUEST_COUNT",
    "requestAttribute": null
  },
  "unit": "COUNT",
  "unitDisplayName": null,
  "entityId": null,
  "managementZones": [],
  "conditions": [
    {
      "attribute": "REQUEST_NAME",
      "comparisonInfo": {
        "type": "STRING",
        "comparison": "EQUALS",
        "value": "{{ .request }}",
        "values": null,
        "negate": false,
        "caseSensitive": false
      }
    },
    {
        "attribute": "SERVICE_TAG",
        "comparisonInfo": {
          "type": "TAG",
          "comparison": "EQUALS",
          "value": {
            "context": "CONTEXTLESS",
            "key": "{{ .tagKey }}",
            "value": "{{ .tagValue }}"
          },
          "negate": false
        }
    },
    {
      "attribute": "RESPONSE_TIME",
      "comparisonInfo": {
        "type": "NUMBER",
        "comparison": "LOWER_THAN_OR_EQUAL",
        "value": {{ .responseTime }},
        "values": null,
        "negate": false
      }
    }
  ]
}